{
  "name": "tiktok-shop-sdk-monorepo",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:examples": "pnpm --filter examples run dev",
    "test": "turbo run test",
    "test:cov": "turbo run test -- --coverage",
    "lint": "turbo run lint",
    "format": "prettier --write \"apps/**/*.{ts,tsx,js,jsx,json,md}\" \"packages/**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky",
    "docs:dev": "vitepress dev apps/docs",
    "docs:build": "vitepress build apps/docs",
    "docs:preview": "vitepress preview apps/docs",
    "lint-staged": "lint-staged --no-stash"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@jest/globals": "^30.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "add": "^2.0.6",
    "eslint": "^9.30.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "pnpm": "^10.12.4",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript-eslint": "^8.35.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hsib19/tiktok-shop-sdk.git"
  },
  "version": "0.0.1"
}
